	"keywords": ["silverstripe", "module", "vendormodule", "cms", "color", "colour", "picker", "form", "field"],
	"homepage": "https://github.com/ninty9notout/silverstripe-colourpicker",
	"license": "MIT",
	"authors": [
		{
			"name": "ninty9notout",
			"homepage": "https://github.com/ninty9notout",
			"email": "ninty9notout@hotmail.co.uk"
		}
	],
	"require": {
        "php": "^8.1",
        "ozdemirburak/iris": "^3.1",
        "silverstripe/framework": "^5.1"
    },
    "autoload": {
        "psr-4": {
            "ninty9notout\\SSColourPicker\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client/dist"
